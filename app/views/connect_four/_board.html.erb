<% @board_render = session[:board] %>
<table>
  <% 5.downto 0 do |row| %>
  <tr>
    <% 0.upto 6 do |column| %>
      <td>
      <% if @board_render[column][row].nil? %>
        -
      <% else %>
        <%= @board_render[column][row] %>
      <% end %>
      </td>
    <% end %>
  </tr>
  <% end %>
  <tr>
    <td>1</td>
    <td>2</td>
    <td>3</td>
    <td>4</td>
    <td>5</td>
    <td>6</td>
    <td>7</td>
  </tr>
</table>